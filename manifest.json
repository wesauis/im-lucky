{
  "name": "im lucky",
  "version": "0.1.0",
  "description": "feeling lucky? let's jump to the first result",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_globs": [
        "$chrome://*"
      ],
      "js": [
        "im-lucky.js"
      ]
    }
  ],
  "permissions": [
    "<all_urls>"
  ]
}