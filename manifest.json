{
  "name": "im lucky",
  "version": "0.2.0",
  "description": "feeling lucky? let's jump to the first result",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "quick-search/google.js"
      ]
    },
    {
      "matches": [
        "https://www.bing.com/search*"
      ],
      "js": [
        "quick-search/bing.js"
      ]
    },
    {
      "matches": [
        "https://duckduckgo.com/*"
      ],
      "js": [
        "quick-search/duckduckgo.js"
      ]
    }
  ],
  "permissions": [
    "https://www.google.com/*",
    "https://www.bing.com/search*",
    "https://duckduckgo.com/*"
  ]
}
